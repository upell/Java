<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="304.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Zakat.View.Login.LoginViewController">
    <stylesheets>
        <URL value="@loginview.css" />
    </stylesheets>
   <children>
      <TextField fx:id="txtIdAmil" alignment="CENTER" layoutX="70.0" layoutY="98.0" prefHeight="32.0" prefWidth="166.0" promptText="ID Amil">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" alignment="CENTER" layoutX="70.0" layoutY="169.0" prefHeight="32.0" prefWidth="166.0" promptText="Password">
         <font>
            <Font size="13.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnMasuk" layoutX="69.0" layoutY="243.0" mnemonicParsing="false" onAction="#prosesLogin" prefHeight="25.0" prefWidth="166.0" text="Masuk" />
      <Button fx:id="btnDaftar" layoutX="70.0" layoutY="277.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="166.0" text="Daftar" />
      <Label fx:id="lblMessageIdAmil" layoutX="83.0" layoutY="129.0" textFill="RED" />
      <Label fx:id="lblMessagePassword" layoutX="82.0" layoutY="200.0" textFill="RED" />
   </children>
</AnchorPane>
